<?xml version="1.0"?>
<!DOCTYPE wallpapers SYSTEM "gnome-wp-list.dtd">
<wallpapers>
  <wallpaper deleted="false">
    <name>Default Background</name>
    <filename>@BACKGROUNDDIR@/vanilla-default.webp</filename>
    <filename-dark>@BACKGROUNDDIR@/vanilla-dark.webp</filename-dark>
    <options>zoom</options>
    <shade_type>solid</shade_type>
    <pcolor>#3071AE</pcolor>
    <scolor>#000000</scolor>
  </wallpaper>
  <wallpaper deleted="false">
    <name>Soft Neon Background</name>
    <filename>@BACKGROUNDDIR@/vanilla-soft.webp</filename>
    <filename-dark>@BACKGROUNDDIR@/vanilla-neon.webp</filename-dark>
    <options>zoom</options>
    <shade_type>solid</shade_type>
    <pcolor>#9988B0</pcolor>
    <scolor>#000000</scolor>
  </wallpaper>
  <wallpaper deleted="false">
    <name>Winter Background</name>
    <filename>@BACKGROUNDDIR@/vanilla-winter-l.png</filename>
    <filename-dark>@BACKGROUNDDIR@/vanilla-winter-d.png</filename-dark>
    <options>zoom</options>
    <shade_type>solid</shade_type>
    <pcolor>#6f81d4</pcolor>
    <scolor>#000000</scolor>
  </wallpaper>
</wallpapers>
